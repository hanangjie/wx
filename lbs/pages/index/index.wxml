<!--index.wxml-->
<view class="container" >
  <button bindtap="startDraw">开始跑步</button>
  <div style="display:{{!debug?'none':''}}" >{{debugMsg}}</div>
  <map id="map" scale="17" controls="{{controls}}" bindcontroltap="controltap" bindmarkertap="markertap" polyline="{{polyline}}"  show-location style="width: 100%; height: 600px;"></map>
</view>
